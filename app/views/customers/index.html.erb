<p id="notice"><%= notice %></p>

<h1>Customers</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Class</th>
      <th>Room</th>
      <th>Building</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>County</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Password</th>
      <th>Credit card</th>
      <th>Credit card type</th>
      <th>Card exp mo</th>
      <th>Card exp yr</th>
      <th>Billing address</th>
      <th>Billing city</th>
      <th>Billing region</th>
      <th>Billing postal code</th>
      <th>Billing country</th>
      <th>Shipping address</th>
      <th>Shipping city</th>
      <th>Shipping region</th>
      <th>Shipping postal code</th>
      <th>Shipping country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.customer_id %></td>
        <td><%= customer.first_name %></td>
        <td><%= customer.last_name %></td>
        <td><%= customer.class %></td>
        <td><%= customer.room %></td>
        <td><%= customer.building %></td>
        <td><%= customer.address1 %></td>
        <td><%= customer.address2 %></td>
        <td><%= customer.city %></td>
        <td><%= customer.county %></td>
        <td><%= customer.phone %></td>
        <td><%= customer.email %></td>
        <td><%= customer.password %></td>
        <td><%= customer.credit_card %></td>
        <td><%= customer.credit_card_type_id %></td>
        <td><%= customer.card_exp_mo %></td>
        <td><%= customer.card_exp_yr %></td>
        <td><%= customer.billing_address %></td>
        <td><%= customer.billing_city %></td>
        <td><%= customer.billing_region %></td>
        <td><%= customer.billing_postal_code %></td>
        <td><%= customer.billing_country %></td>
        <td><%= customer.shipping_address %></td>
        <td><%= customer.shipping_city %></td>
        <td><%= customer.shipping_region %></td>
        <td><%= customer.shipping_postal_code %></td>
        <td><%= customer.shipping_country %></td>
        <td><%= link_to 'Show', customer %></td>
        <td><%= link_to 'Edit', edit_customer_path(customer) %></td>
        <td><%= link_to 'Destroy', customer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer', new_customer_path %>
