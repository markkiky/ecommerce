<%# stylesheet_link_tag "mpesa_modal" %>
<!-- Modal -->
<div class="modal fade" id="mpesaPushSentModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="mpesaModalTitle">Push Sent</h5>
        <button type="button" onclick="close_mpesa_push_sent()" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="col-12">
          <%= image_tag 'check_phone', width: "400px" %>
        </div>
        <div class="response">
            <p>Enter Phone Number</p>
        </div>
        </div>
      </div>
      <div class="modal-footer d-none">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>

